    "eslint-plugin-svelte": "^3.12.4",
    "globals": "^16.4.0",
    "mdsvex": "^0.12.6",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "prettier-config": "github:boyum/prettier-config",
    "storybook": "^10.0.4",
    "supabase": "^2.54.11",
    "svelte": "^5.41.0",
    "svelte-check": "^4.3.3",
    "typescript": "^5.9.3",
